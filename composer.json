{
  "name": "effina/larabanner",
  "description": "A simple banner management system for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "effina",
      "email": "tech@effina.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Yourusername\\Larabanner\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yourusername\\Larabanner\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yourusername\\Larabanner\\LarabannerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
